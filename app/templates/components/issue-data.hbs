<div id="table-div">
    <table id='data-table' class='table table-striped table-bordered'>
        <thead>
            <tr>
                <th>Submit Time</th>
                <th>Cust Name</th>
                <th>Cust Email</th>
                <th>Description</th>
                <th>Open/Closed</th>
                <th>Closed Time</th>
                <th>Employee Assigned</th>
            </tr>
        </thead>
        <tbody>
            {{#each issues as |issue|}}
                <tr>
                    {{#each issue as |column|}}
                        <td class=''>{{column}}</td>
                    {{/each}}
                </tr>
            {{/each}}
        </tbody>
    </table>
</div>

{{yield}}
